{
  "name": "client3",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "babel src --out-dir lib --extensions \".ts,.tsx\"",
    "build:watch": "babel src --watch --out-dir www/js --extensions \".ts,.tsx\"",
    "dev": "concurrently \"npm run type-check:watch\" \"npm run build:watch\""
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/express": "^4.17.13",
    "babel-preset-solid": "^1.4.2",
    "concurrently": "^7.2.1",
    "dotenv": "^16.0.1",
    "socket.io-client": "^4.5.1",
    "solid-js": "^1.4.3",
    "typescript": "^4.7.3"
  },
  "dependencies": {}
}
